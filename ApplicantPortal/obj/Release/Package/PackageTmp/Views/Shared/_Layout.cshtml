<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
   
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-3">

                    <img src="~/Content/Images/newlogo.png" />
                </div>
                <div class="col-md-5">
                    <div class="row">
                        <div class="col-md-12">
                            <h2 class="text-center">Common Application Form</h2>
                        </div>
                        <div class="col-md-12">
                            <h4 class="text-center">For Admission to Intermediate Courses Session 2019-21</h4>
                        </div>
                        <div class="col-md-12">
                            <h6 class="text-center">Bihar School Examination Board, Government of Bihar. </h6>
                        </div>
                    </div>
                </div>
            </div>
        </div>
   
    <div class="container body-content">
        @RenderBody()
       
        
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
